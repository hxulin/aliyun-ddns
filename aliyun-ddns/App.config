<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- 阿里云密钥ID -->
    <add key="ACCESS_KEY_ID" value="test_id"/>
    <!-- 阿里云密钥 -->
    <add key="ACCESS_KEY_SECRET" value="test_key"/>
    <!-- 域名 -->
    <add key="DOMAIN_NAME" value="test.com"/>
    <!-- 子域名 -->
    <add key="SUBDOMAIN_NAME" value="www"/>

    <!-- 本地的DNS网关地址 -->
    <add key="LOCAL_DNS_ADDRESS" value="192.168.1.1"/>
    <!-- 运行时出现错误重试的次数 -->
    <add key="ERROR_RETRIES" value="5"/>
  </appSettings>
</configuration>